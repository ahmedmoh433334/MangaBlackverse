{
  "buildCommand": "next build",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "nodeVersion": "20.x",
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": {
      "description": "Your Supabase project URL",
      "required": true
    },
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": {
      "description": "Your Supabase anon public key",
      "required": true
    }
  },
  "functions": {
    "server.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
